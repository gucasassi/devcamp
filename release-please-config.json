{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-type": "node",
  "pull-request-title-pattern": "chore(release): v${version}",
  "pull-request-header": "🚀 Preparing release ${version}",
  "pull-request-footer": "This PR was automatically generated by release-please 🤖",
  "packages": {
    ".": {
      "package-name": "devcamp",
      "initial-version": "1.0.0"
    }
  },
  "changelog-sections": [
    {
      "type": "feat",
      "section": "✨ Features"
    },
    {
      "type": "fix",
      "section": "🐛 Bugs"
    },
    {
      "type": "chore",
      "hidden": true,
      "section": "🧹 Chore"
    },
    {
      "type": "docs",
      "hidden": true,
      "section": "📚 Documentation"
    },
    {
      "type": "perf",
      "section": "🚀 Performance"
    },
    {
      "type": "build",
      "section": "🛠️ Build"
    },
    {
      "type": "ci",
      "hidden": true,
      "section": "🚦 CI"
    },
    {
      "type": "refactor",
      "section": "🔄 Refactors"
    },
    {
      "type": "revert",
      "section": "🔙 Reverts"
    },
    {
      "type": "style",
      "section": "🎨 Styling"
    },
    {
      "type": "test",
      "hidden": true,
      "section": "🧪 Tests"
    }
  ]
}
