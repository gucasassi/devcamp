{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "bootstrap-sha": "d9604d9275a74156cfc4da38bcd88ad80083fb16",
  "pull-request-title-pattern": "chore(release): v${version}",
  "pull-request-header": "This pull request prepares a new release.",
  "pull-request-footer": "Generated automatically by release-please 🤖",
  "packages": {
    ".": {
      "release-type": "node",
      "include-component-in-tag": false
    }
  },
  "changelog-sections": [
    {
      "type": "feat",
      "section": "✨ Features"
    },
    {
      "type": "fix",
      "section": "🐛 Bugs"
    },
    {
      "type": "chore",
      "hidden": true,
      "section": "🧹 Chore"
    },
    {
      "type": "docs",
      "hidden": true,
      "section": "📚 Documentation"
    },
    {
      "type": "perf",
      "section": "🚀 Performance"
    },
    {
      "type": "build",
      "section": "🛠️ Build"
    },
    {
      "type": "ci",
      "hidden": true,
      "section": "🚦 CI"
    },
    {
      "type": "refactor",
      "section": "🔄 Refactors"
    },
    {
      "type": "revert",
      "section": "🔙 Reverts"
    },
    {
      "type": "style",
      "section": "🎨 Styling"
    },
    {
      "type": "test",
      "hidden": true,
      "section": "🧪 Tests"
    }
  ]
}
