  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/gucasassi/devcamp/issues"
  },
  "homepage": "https://github.com/gucasassi/devcamp#readme",
  "dependencies": {
    "colors": "1.4.0",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "mongoose": "8.19.2",
    "morgan": "1.10.1",
    "node-geocoder": "4.4.1",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
